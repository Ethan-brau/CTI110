def main
  print ("What year was the console assembled?")
  year = int(input())
  while year < 2011:
    print('Likely a Corona Board... let us get some more info!\t\t')
    
  while year > 2011:
    print('Likely a Trinity Board... Great! let us get some more info!\t\t')

def month
  print ("What month was the console assembled?")
  mnfmonth = int(input())
  while mnfmonth < 7:
    print('this is a corona board... what size nand? if boxed in with white lines, 4mb. if no white lines, 16mb.')
   while mnfmonth > 8:
